{
  "name": "@tipe/ui",
  "version": "2.0.4",
  "main": "build/lib/index.js",
  "files": [
    "dist",
    "LICENSE",
    "build/lib/index.js",
    "package.json"
  ],
  "license": "MIT",
  "scripts": {
    "build": "node build/build.js && yarn webpack --config build/configs/webpack.prod.conf.js",
    "build:dev": "node build/build.js && yarn webpack --config build/configs/webpack.dev.conf.js",
    "watch:build:dev": "node build/watch.js",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "ci": "yarn lint && yarn test",
    "precommit": "lint-staged && yarn lint && yarn test",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once --pro"
  },
  "dependencies": {
    "@tipe/tipe-config": "^1.0.0",
    "@tipe/tipe-constants": "^1.0.0",
    "@tipe/tipe-interfaces": "^1.0.4-0",
    "autosize": "^4.0.2",
    "chokidar": "^2.0.4",
    "storybook": "^1.0.0",
    "vue": "^2.5.16",
    "vue-click-outside": "^1.0.7",
    "vue-types": "^1.2.3",
    "xstate": "^3.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.47",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.1",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-centered": "^3.4.5",
    "@storybook/addon-knobs": "^3.4.4",
    "@storybook/addon-links": "^3.4.4",
    "@storybook/addon-notes": "^3.4.4",
    "@storybook/addons": "^3.4.4",
    "@storybook/storybook-deployer": "^2.3.0",
    "@storybook/vue": "^3.4.4",
    "@tipe/tipe-test-utils": "^1.0.2",
    "@vue/cli-plugin-babel": "^3.0.0-beta.10",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.10",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.10",
    "@vue/cli-service": "^3.0.0-beta.10",
    "@vue/eslint-config-prettier": "^3.0.0-beta.10",
    "@vue/test-utils": "^1.0.0-beta.22",
    "autoprefixer": "^8.5.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "eslint-plugin-vue-types": "^0.1.0",
    "faker": "^4.1.0",
    "file-loader": "^1.1.4",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "postcss": "^6.0.22",
    "postcss-color-function": "^4.0.1",
    "postcss-for": "^2.1.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-mixins": "^6.2.0",
    "postcss-nesting": "^5.0.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.9",
    "style-loader": "^0.21.0",
    "svg-sprite-loader": "^3.7.3",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "travis-deploy-once": "^5.0.2",
    "uuid": "^3.2.1",
    "vue-loader": "^13.0.5",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.6.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{vue,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "assets": [
      "package.json",
      "CHANGELOG.md",
      "LICENSE",
      "build/lib/index.js"
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        }
      ],
      "parserOpts": {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES",
          "BREAKING"
        ]
      }
    }
  }
}
