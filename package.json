{
  "name": "@tipe/ui",
  "version": "2.2.3",
  "main": "dist/tipe-ui.common.js",
  "module": "src/main.js",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "CHANGELOG.md",
    "README.md"
  ],
  "keywords": [
    "tipe",
    "vue",
    "ui",
    "components"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "build:bundle": "vue-cli-service build --target lib --name tipe-ui src/main.js",
    "test": "vue-cli-service test:unit",
    "update-snaps": "yarn test --updateSnapshot",
    "ci": "yarn lint && yarn test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "precommit": "lint-staged && yarn lint && yarn test",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once --pro"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "vue": "^2.5.17",
    "vue-click-outside": "^1.0.7",
    "vue-markdown": "^2.2.4",
    "vue-types": "^1.2.3",
    "xstate": "^3.2.1"
  },
  "devDependencies": {
    "chokidar": "^2.0.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "storybook": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.47",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.1",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-centered": "^3.4.5",
    "@storybook/addon-knobs": "^3.4.4",
    "@storybook/addon-links": "^3.4.4",
    "@storybook/addon-notes": "^3.4.4",
    "@storybook/addons": "^3.4.4",
    "@storybook/storybook-deployer": "^2.3.0",
    "@storybook/vue": "^3.4.4",
    "@tipe/eslint-config-tipe": "^2.0.0",
    "@tipe/tipe-config": "^1.0.0",
    "@tipe/tipe-constants": "^1.0.0",
    "@tipe/tipe-test-utils": "^1.0.4",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-preset-vue": "^2.0.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue-types": "^0.2.0",
    "faker": "^4.1.0",
    "lint-staged": "^7.2.2",
    "postcss-color-function": "^4.0.1",
    "postcss-for": "^2.1.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-mixins": "^6.2.0",
    "postcss-nesting": "^5.0.0",
    "storybook": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-sprite-loader": "^3.7.3",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "autoprefixer": {},
      "postcss-nesting": {},
      "postcss-mixins": {},
      "postcss-for": {}
    }
  },
  "release": {
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "CHANGELOG.md",
          "LICENSE",
          "build/lib/index.js",
          "README.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        }
      ],
      "parserOpts": {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES",
          "BREAKING"
        ]
      }
    }
  }
}
